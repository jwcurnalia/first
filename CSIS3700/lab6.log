#include <iostream>

using namespace std;

void BubbleSort(int Array[], int n)
{
bool done = false;
int temp, counter=1;

while(!done) 
     {
     done = true;

     for(int i=0; i<n-1; i++) 
        {
        if(Array[i] > Array[i+1]) 
          {
          temp = Array[i];
          Array[i] = Array[i+1];
          Array[i+1] = temp;
          done = false;
          }
        }
        cout << "After pass " << counter << ":";
        for(int j=0; j<n; j++)
           cout << Array[j] << " ";
        counter++;
        cout << endl;
     }
}

int main() {

int num = 10;
int array1[10] = {13, 21, 6, 84, 9, 55, 35, 27, 16, 40 }; 
int array2[10] = {9, 6, 16, 21, 13, 35, 27, 40, 84, 55 };

cout << "For Array 1: " << endl;
BubbleSort(array1, num);

cout << "For Array 2: " << endl;
BubbleSort(array2, num);

return 0;
}

