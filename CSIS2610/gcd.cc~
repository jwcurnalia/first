#include <iostream>

using namespace std;

int main(void) {
  int m,n,r,a,b;

  cout << "Enter m and n: ";
  cin >> m >> n;

  a = m;
  b = n;

  while (n != 0) {
    r = m % n;
    m = n;
    n = r;
  }

  cout << "GCD of " << a << " and " << b << " is " << m << endl;
// printf("GCD of %d and %d is %d\n",m,n,m);

  return 0;
}
